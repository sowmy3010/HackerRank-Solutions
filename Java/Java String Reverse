import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B="";
        int l=A.length();
        for(int i=0;i<l;i++)
        {
            char ch=A.charAt(i);
            B=ch+B;
        }
        if(A.compareTo(B)==0)
        System.out.println("Yes");
        else
        System.out.println("No");
    }
}



